{
    "name": "gzero/cms",
    "description": "Gzero CMS",
    "keywords": ["framework", "laravel", "cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.2.11",
        "gzero/eloquent-tree": "dev-master",
        "robclancy/presenter": "1.2.0",
        "machuga/authority-l4": "2.1.0",
        "barryvdh/laravel-twigbridge": "dev-master",
        "bigecko/laravel-theme": "dev-master"
    },
    "replace": {
        "gzero/core": "self.version",
        "gzero/entity": "self.version",
        "gzero/repository": "self.version",
        "gzero/validator": "self.version"
    },
    "require-dev": {
        "fzaninotto/faker": "1.4.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.3.*",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Gzero\\": "src/Gzero"
        },
        "classmap": [
            "src/migrations"
        ]
    },
    "minimum-stability": "dev"
}
