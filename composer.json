{
    "name": "gzero/cms",
    "description": "Gzero CMS",
    "keywords": [
        "framework",
        "laravel",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "davejamesmiller/laravel-breadcrumbs": "3.0.*",
        "filp/whoops": "2.1.*",
        "gzero/eloquent-tree": "3.*",
        "laravel/framework": "5.3.*",
        "laravel/passport": "^1.0",
        "robclancy/presenter": "1.3.1"
    },
    "replace": {
        "gzero/core": "self.version",
        "gzero/entity": "self.version",
        "gzero/repository": "self.version",
        "gzero/validator": "self.version"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "^1.0",
        "aedart/testing-laravel": "1.8.*",
        "gzero/testing": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Gzero\\": "src/Gzero"
        },
        "classmap": [
            "src/Gzero/Core/database/migrations"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "./vendor/bin/codecept run"
    },
    "minimum-stability": "stable"
}
